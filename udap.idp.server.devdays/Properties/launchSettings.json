{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "UdapIdpBaseUrl": "https://localhost:5202"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:5202"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,      
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "UdapIdpBaseUrl": "https://localhost:5202"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:5202"
    },   
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "5202",
        "UdapIdpBaseUrl": "https://localhost:5202"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}